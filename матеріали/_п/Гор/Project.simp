        <operation id="o2ac401be2954486284236e8b52fce0f3228" cid="OP141" name="scrollToTab" type="" type-id="" visibility="public">
          <style class="default" />
          <parameters>
            <parameter id="pbe4328cc636c4bc8b15aa47637d5176b" name="tabIndex" type="int" type-id="int" />
