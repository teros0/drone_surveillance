        <operation id="ob04328d91ac4402aa31976e8fb8e7d2e228" cid="OP149" name="blendColors" type="int" type-id="int" visibility="private" static="true">
          <style class="default" />
          <parameters>
            <parameter id="pbab93b6f7f33498794e7916cc47fd894" name="color1" type="int" type-id="int" />
