        <operation id="o12adcbc0dd644a9ab43fa167a0006acc223" cid="OP014" name="encode" type="byte[]" type-id="byte[]" visibility="public" static="true">
          <style class="default" />
          <parameters>
            <parameter id="p7096f1f0b92a4696b93cfe2bf7428c6d" name="input" type="byte[]" type-id="byte[]" />
            <parameter id="p8cf67e79a070417bbb0696745ec8c430" name="offset" type="int" type-id="int" />
