                JSONObject json = jsonParser.makeHttpRequest(
                        LOGIN_URL, "GET", reqString);
                
                Log.d("lol", json.toString());
                if (json.getInt("StatusCode") == 200) {
                    MainActivity.sessionId = json.getString("Data");
                    reqString.clear();
