        <operation id="o310ccdfb773f4df0ac1b74c303ebeedf202" cid="OP321" name="blendColors" type="int" type-id="int" visibility="private" static="true">
          <style class="default" />
          <parameters>
            <parameter id="p71cd264008354a679c44d890e3f93c08" name="color1" type="int" type-id="int" />
