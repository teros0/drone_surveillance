        Decoder decoder = new Decoder(flags, new byte[len * 3 / 4]);

        if (!decoder.process(input, offset, len, true)) {
            throw new IllegalArgumentException("bad base-64");
        }

        
        if (decoder.op == decoder.output.length) {
            return decoder.output;
        }

        
        
